#include <stdio.h>
#include <stdlib.h>
#include<time.h>

int main()
{
    int num,guess,tries=0,n;
    srand(time(0));
    printf("Enter The Last Number:");
    scanf("%d",&n);
    printf("The Number Range 1 to %d=",n);
    for(int i=1;i<=n;i++)
    {
      printf("%d\t",i);
    }
    printf("\n");

    num=rand()%n+1;
    printf("\t\tNumber Guessing Game\n");
    printf("\t\t--------------------\n\n");
    do
    {
        printf("Enter The Guess Number:");
        scanf("%d",&guess);
        tries++;
        if(guess>num)
        {
            printf("Too high!\n\n");
        }
        else if(guess<num)
        {
            printf("Too low!\n\n");
        }
        else
            printf("\nCorrect! You got it in %d guesses!\n", tries);
    }
    while(guess!=num);
    return 0;

}
